{
    "name": "netgalley/oauth1-goodreads",
    "description": "OAuth 1.0 Client Library for Goodreads",
    "homepage": "https://github.com/fbng/oauth1-goodreads",
    "license": "MIT",
    "authors": [
        {
            "name": "Israel J. Carberry",
            "email": "israel.carberry@netgalley.com"
        }
    ],
    "keywords": [
        "oauth",
        "oauth1",
        "authorization",
        "authentication",
        "idp",
        "identity",
        "sso",
        "single sign on",
        "goodreads"
    ],
    "require": {
        "php": ">=5.5.0",
        "league/oauth1-client": "^1.7"
    },
    "require-dev": {
        "php": "^7.0",
        "phpunit/phpunit": "^6.3"
    },
    "autoload": {
        "psr-4": { "NetGalley\\OAuth1\\Client\\Server\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "NetGalley\\OAuth1\\Client\\Tests\\Server\\": "tests/src/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
